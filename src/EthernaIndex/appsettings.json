{
  "AllowedHosts": "*",

  "Application": {
    "CompactName": "ethernaIndex"
  },

  "SsoServer": {
    "Clients": {
      "Webapp": {
        "ClientId": "ethernaIndexWebappClientId"
      }
    },
    "LoginPath": "/Identity/Account/Login",
    "RegisterPath": "/Identity/Account/Register"
  },

  "Swarm": {
    "GatewayUrl": "https://gateway.etherna.io/"
  },

  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    }
  }
}
